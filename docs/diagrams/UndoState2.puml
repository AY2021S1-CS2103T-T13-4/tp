@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title After command add c/expense..."

package States <<rectangle>> {
    class State1 as "__aa1Copy:ActiveAccount__"
    class State2 as "__aa2Copy:ActiveAccount__"
    class State3 as "__aa3:ActiveAccountManager__"
}

State1 -[hidden]right-> State2
State2 -[hidden]right-> State3

class Pointer as "Current State" #FFFFF

Pointer -up-> State3
State2 -left-> State1
State2 -[hidden]left-> State1
State3 -left-> State2
State3 -[hidden]left-> State2

@end
